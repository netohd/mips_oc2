module MUXhazard2 (entradaA, entradaB, entradaC, controle, saida);
    input wire [31:0] entradaA, entradaB, entradaC;
    input wire [1:0] controle; //sinal seletor
    output [31:0] Saida; // saida 32-bit
    case (controle)
      0: assign saida = entradaA;
      1: assign saida = entradaB;
      2: assign saida = entradaC;
      default: saida = entradaA;
    endcase
endmodule